{
  "name": "crowdin-app-template",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "bun run build && wrangler dev --port ${PORT:-3000}",
    "build": "rm -rf build && tsc --noEmitOnError false",
    "lint": "eslint src --ext .ts,.tsx --format json",
    "deploy": "bun run build && wrangler deploy",
    "preview": "bun run build && wrangler dev",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@crowdin/app-project-module": "^0.96.3",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/express": "^5.0.0",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.42.1"
  },
  "overrides": {
    "express": "^5.1.0"
  }
}
